{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "norrehus/masonry-grid",
  "version": "0.1.0",
  "title": "Masonry Grid",
  "category": "design",
  "icon": "grid-view",
  "description": "Show a gallery as masonry frid",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "masonry-grid",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "layout": {
      "type": "string",
      "default": "layout_1"
    },
    "images": {
      "type": "array",
      "default": []
    },
    "color": {
      "type": "string",
      "default": ""
    },
    "contentPosition": {
      "type": "string",
      "default": "middle-center"
    }
  },
  "providesContext": {
    "norrehus/color": "color",
    "norrehus/contentPosition": "contentPosition"
  }
}